next()
}
else {
result.temp <- t.test(rater[,i], fmri[,i], vvar.equal = TRUE)
ttestResults[i-2, 1:3] <- c(result.temp$statistic[["t"]], result.temp$parameter[["df"]], result.temp$p.value)
}
View(result.temp)
